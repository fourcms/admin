{
    "name": "fourcms/admin",
    "type": "library",
    "description": "Admin package for fourcms",
    "keywords": ["fourcms"],
    "license": "proprietary",
    "authors": [
        {
            "name": "Avtandil Kikabidze aka LONGMAN",
            "email": "akalongman@gmail.com",
            "homepage": "http://longman.me",
            "role": "Maintainer, Developer"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.1.*|5.2.*",
        "longman/laravel-platfourm": "~0.0.1",
        "fourcms/auth": "~1.0.0"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.8",
        "phpspec/phpspec": "~2.5",
        "squizlabs/php_codesniffer": "2.6.*",
        "graham-campbell/testbench": "~3.1"
    },
    "autoload": {
        "psr-4": {
            "FourCms\\Admin\\": "src/Admin"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.itdc.ge"
        }
    ]
}
